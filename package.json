{
  "name": "@andrew-codes/color-functions",
  "version": "1.4.0",
  "description": "Collection of functions to manipulate colors in JavaScript",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js --ignore-pattern *.spec.js",
    "test": "cross-env NODE_ENV=test node_modules/.bin/mocha --ui tdd --compilers js:babel-core/register src/**/*.spec.js --recursive",
    "build": "babel src --out-dir dist"
  },
  "keywords": [
    "color"
  ],
  "author": "Andrew Smith <andrew@andrew.codes>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.11.1",
    "eslint-config-andrew-codes": "^1.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3",
      "stage-2",
      "stage-1"
    ],
    "env": {
      "production": {
        "ignore": [
          "**/*.spec.js"
        ],
        "minified": true
      }
    }
  }
}
